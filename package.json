    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucthesloth/maplinkv2.git"
  },
  "keywords": [],
  "author": "",
